Cell[CellGroupData[{

Cell["Timing am Beispiel y'\[Equal]Cos(x)y", "Section",
  ShowGroupOpenCloseIcon->True],

Cell[BoxData[
    \(AdaptiveExplizitRungeKuttaTime[Xo_, Yo_, c_, epsabs_, epsrel_, ho_, 
        f1_, f2_, p1_, sigma_, lamdaMin_, lamdaMax_] := 
      Module[{ret, hk, q, yp1, yp2, epsk, lamdak, k}, ret = {{Xo, Yo}}; 
        hk = ho; q = p1; 
        k = 1; \[IndentingNewLine]While[
          ret[\([\)\(k, 1\)\(]\)] + hk < Xo + c, \[IndentingNewLine]yp1 = 
            ret[\([\)\(k, 2\)\(]\)] + 
              f1[ret[\([\)\(k, 1\)\(]\)], ret[\([\)\(k, 2\)\(]\)], hk]*hk; 
          yp2 = ret[\([\)\(k, 2\)\(]\)] + 
              f2[ret[\([\)\(k, 1\)\(]\)], ret[\([\)\(k, 2\)\(]\)], hk]*
                hk; \[IndentingNewLine]epsk = \((epsabs + 
                  epsrel*Max[yp1, ret[\([\)\(k, 2\)\(]\)]])\)*hk\/c; 
          lamdak = 
            If[yp1 \[NotEqual] \ yp2, \((epsk/Abs[yp2 - yp1])\)\^\(1/q\), 
              lamdaMax]; \[IndentingNewLine]If[
            lamdak \[GreaterEqual] \ 1 - sigma, 
            AppendTo[
              ret, {Min[ret[\([\)\(k, 1\)\(]\)] + hk, Xo + c], 
                yp2}]; \[IndentingNewLine]hk = 
              If[lamdak \[GreaterEqual] \ 1 + sigma, 
                Min[lamdaMax, lamdak]*hk, hk]; \(k++\), 
            hk = Max[Abs[lamdaMin], Abs[lamdak]]*hk]]; ret]\)], "Input"],

Cell[BoxData[
    \(PossibleH[l_, n_] := 
      Module[{ret, xlist, hmin, hmax, 
          k}, \[IndentingNewLine]xlist = \(Transpose[l]\)[\([1]\)]; 
        hmin\  = \(hmax = 
            xlist[\([2]\)] - xlist[\([1]\)]\); \[IndentingNewLine]For[k = 2, 
          k \[LessEqual] \ Length[xlist], \(k++\), \[IndentingNewLine]If[
            hmin\  > xlist[\([k]\)] - xlist[\([k - 1]\)], 
            hmin\  = 
              xlist[\([k]\)] - xlist[\([k - 1]\)]]; \[IndentingNewLine]If[
            hmax\  < xlist[\([k]\)] - xlist[\([k - 1]\)], 
            hmax\  = 
              xlist[\([k]\)] - xlist[\([k - 1]\)]];\[IndentingNewLine]]; 
        Range[hmin, hmax, \((hmax - hmin)\)/n]]\)], "Input"],

Cell[BoxData[{
    \(x0 = 0. ; y0 = 1. ; c = 12. ; eabs = 0.001; erel = 0.0; h = 0.037; 
    p1 = 2;\), "\[IndentingNewLine]", 
    \(sigma = 0.5; lamdaMin = 0.2; lamdaMax = 1.6;\), "\[IndentingNewLine]", 
    \(f[x_, y_] := Cos[x]*y; \[Phi]1[x_, y_, h_] := 
      1\/2*f[x, y] + 1\/2*f[x + h, y + h*f[x, y]];\), "\[IndentingNewLine]", 
    \(\(\[Phi]2[x_, y_, h_] := 
        1\/6*f[x, y] + 1\/6*f[x + h, y + h*f[x, y]] + 
          2\/3*f[x + h\/2, 
              y + h\/4*f[x, y] + h\/4*f[x + h, y + h*f[x, y]]];\)\)}], "Input"],

Cell[BoxData[{
    \(\(lsg1 = 
        Timing[AdaptiveExplizitRungeKuttaTime[x0, y0, c, eabs, erel, 
            h, \[Phi]1, \[Phi]2, p1, sigma, lamdaMin, 
            lamdaMax]];\)\), "\[IndentingNewLine]", 
    \(posh = PossibleH[time1[\([2]\)], 60]; 
    timeList\  = \ {};\), "\[IndentingNewLine]", 
    \(\(plot1 = 
        ListPlot[{{posh[\([1]\)], lsg1[\([1]\)]/Second}}, 
          DisplayFunction \[Rule] Identity, 
          PlotStyle \[Rule] {PointSize[0.02], RGBColor[1, 0, 0]}, 
          PlotRange \[Rule] All];\)\), "\[IndentingNewLine]", 
    \(For[i = 1, i \[LessEqual] \ Length[posh], \(i++\), 
      AppendTo[timeList, \[IndentingNewLine]{posh[\([i]\)], \(Timing[
                ExplicitRungeKutta[x0, y0, 
                  Stuetzstellen[x0, \((x0 + c)\), 
                    posh[\([i]\)]], \[Phi]1]]\)[\([1]\)]/
            Second}\[IndentingNewLine]]]; 
    plot2 = ListPlot[timeList, DisplayFunction \[Rule] Identity, 
        PlotJoined \[Rule] True, PlotRange \[Rule] All];\), "\n", 
    \(\(Show[plot1, plot2, DisplayFunction \[Rule] $DisplayFunction, 
        FrameLabel \[Rule] {"\<h\>", "\<Time\>"}, RotateLabel -> True, 
        PlotLabel \[Rule] "\<Zeitbetrachtungen\>"];\)\)}], "Input",
  ShowClosedCellArea->True]
}, Closed]]
}, Closed]],

