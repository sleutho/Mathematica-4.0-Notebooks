Cell[BoxData[
    \(\(\(AdaptiveExplizitRungeKutta[Xo_, Yo_, c_, epsabs_, epsrel_, ho_, 
        f1_, f2_, p1_, p2_, sigma_, lamdaMin_, lamdaMax_, nf_, 
        y_]\)\(:=\)\(Module[{ret, k, hk, q, yp1, yp2, epsk, lamdak, hlist, 
          declinehlist, numberf, localepslist, 
          declineh}, \[IndentingNewLine]ret = {{Xo, Yo}}; hk = ho; q = p1; 
        k = 1; hlist = \(declinehlist = \(localepslist = {0. }\)\); \
\[IndentingNewLine]declineh = 0; numberf = 0; 
        apsepslist = \(relepslist = {0. }\); \[IndentingNewLine]While[
          ret[\([k, 1]\)] + hk < Xo + c, \[IndentingNewLine]yp1 = 
            ret[\([k, 2]\)] + 
              f1[ret[\([k, 1]\)], ret[\([k, 2]\)], hk]*
                hk; \[IndentingNewLine]yp2 = 
            ret[\([k, 2]\)] + f2[ret[\([k, 1]\)], ret[\([k, 2]\)], hk]*hk; 
          numberf += 
            nf; \[IndentingNewLine]epsk = \((epsabs + 
                  epsrel*Max[yp1, ret[\([k, 2]\)]])\)*
              hk\/c; \[IndentingNewLine]lamdak = 
            If[yp1 \[NotEqual] \ yp2, \@\(epsk\/Abs[yp2 - yp1]\)\%q, 
              lamdaMax]; \[IndentingNewLine]If[
            lamdak \[GreaterEqual] \ 1 - sigma, \[IndentingNewLine]AppendTo[
              ret, {Min[ret[\([k, 1]\)] + hk, Xo + c], 
                yp2}]; \[IndentingNewLine]hk = 
              If[lamdak \[GreaterEqual] \ 1 + sigma, 
                Min[lamdaMax, lamdak]*hk, hk]; \[IndentingNewLine]AppendTo[
              hlist, hk]; AppendTo[declinehlist, declineh]; 
            AppendTo[localepslist, 
              Abs[yp2 - yp1]]; \[IndentingNewLine]AppendTo[apsepslist, 
              y[ret[\([k, 1]\)]] - ret[\([k, 2]\)]]; 
            AppendTo[
              relepslist, \((y[ret[\([k, 1]\)]] - ret[\([k, 2]\)])\)/
                y[ret[\([k, 1]\)]]]; \[IndentingNewLine]\(k++\); 
            declineh = 0, \[IndentingNewLine]hk = 
              Max[Abs[lamdaMin], Abs[lamdak]]*
                hk; \(declineh++\)] (*end\ If*) \[IndentingNewLine]] (*end\ \
while*) ; \[IndentingNewLine]{ret, hlist, declinehlist, numberf, 
          localepslist, apsepslist, 
          relepslist}]\)\( (*end\ module*) \)\)\)], "Input"],

Cell[BoxData[
    \(\(\(passiveExtrapolation[Xo_, Yo_, C_, f_, p_, K1_, eps_, 
        y_]\)\(:=\)\(Module[{ord, yk, ykk, I, J, K, i, k, st, ret, h, L, 
          maxi}, \[IndentingNewLine]ret = {}; ord = {}; K = K1; maxi = 20; 
        If[eps > 1, maxi = eps, ]; \[IndentingNewLine]For[k = 1, 
          k \[LessEqual] maxi, \ \(k++\), \[IndentingNewLine]st = 
            Stuetzstellen[Xo, Xo + C, C/2. \^\(K - 1\)]; 
          yk = \(Last[
                ExplicitRungeKutta[Xo, Yo, st, 
                  f]]\)[\([2]\)]; \[IndentingNewLine]AppendTo[
            ret, {yk}]; \[IndentingNewLine]For[i = 2, 
            i \[LessEqual] k, \ \(i++\), 
            ykk = \((2\^\(p + i - 2\)*ret[\([k, i - 1]\)] - 
                    ret[\([k - 1, i - 1]\)])\)/\((2\^\(p + i - 2\) - 
                    1)\); \[IndentingNewLine]AppendTo[ret[\([k]\)], ykk]; 
            If[eps < 1 && Abs[ykk - yk] < eps, maxi = \(-1\); Break, 
              yk = ykk];\[IndentingNewLine]] (*end\ for*) ; \(K++\);\
\[IndentingNewLine]] (*end\ for*) ; 
        L = Length[ret // Last]; \[IndentingNewLine]For[I = 1, 
          I < L, \(I++\), 
          For[J = I, 
            J < L, \(J++\), \[IndentingNewLine]AppendTo[
              ret[\([I]\)], "\<-\>"]]]; \[IndentingNewLine]L = Length[ret]; 
        For[I = 1, 
          I < L, \(I++\), \[IndentingNewLine]AppendTo[ord, 
            Log[10, Abs[\(Last[ret]\)[\([I]\)] - y[Xo + \
C]]\/Abs[\(ret[\([Length[ret] - 1]\)]\)[\([I]\)] - y[Xo + C]]]\/Log[10, \
\(C\/2. \^k\)\/\(C\/2. \^\(k - 1\)\)]]]; 
        AppendTo[ord, "\<-\>"]; \[IndentingNewLine]AppendTo[ret, 
          ord]\[IndentingNewLine]]\)\( (*end\ module*) \)\)\)], "Input"],

Cell[BoxData[
    \(\(\(activeExtrapolation[Xo_, Yo_, C_, Hlist_, f_, p_, 
        u]\)\(:=\)\(Module[{xo, yo, i, ret}, \[IndentingNewLine]ret = {}; 
        xo = Xo; 
        yo = Yo; \[IndentingNewLine]For[i = 1, 
          i \[LessEqual] 
            C/Hlist[\([1]\)], \ \(i++\), \[IndentingNewLine]AppendTo[ret, 
            Take[passiveExtrapolation[xo, yo, Hlist[\([1]\)], f, p, 1, 3, u], 
              3, 3]]; \[IndentingNewLine]xo += Hlist[\([1]\)]; 
          yo = \(ret[\([i]\)]\)[\([3, 
                3]\)]\[IndentingNewLine]] (*end\ for*) ; \
\[IndentingNewLine]NumberForm[ret, 
          30]\[IndentingNewLine]]\)\( (*end\ module*) \)\)\)], "Input"]
}, Closed]],

