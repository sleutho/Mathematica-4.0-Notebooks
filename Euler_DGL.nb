Cell[BoxData[
    \(\(\(ExplicitRungeKutta[Xo_, Yo_, H_, f_]\)\(:=\)\(Module[{N, ret, k}, 
        N = Length[H]; 
        ret = {{Xo, Yo}}; \[IndentingNewLine]For[k = 1, 
          k \[LessEqual] 
            N, \(k++\), \[IndentingNewLine]\(AppendTo[
              ret, {ret[\([k, 1]\)] + H[\([k]\)], 
                ret[\([k, 2]\)] + 
                  f[ret[\([k, 1]\)], ret[\([k, 2]\)], H[\([k]\)]]*
                    H[\([k]\)]}];\)\[IndentingNewLine]] (*end\ for*) ; \
\[IndentingNewLine]ret]\)\( (*end\ module*) \)\)\)], "Input"],

Cell[BoxData[
    \(\(\(ImplicitRungeKutta[Xo_, Yo_, H_, f_, alpha_, gamma_, 
        betta_]\)\(:=\)\(Module[{N, ret, k, F, X, i, w}, N = Length[H]; 
        ret = {{Xo, Yo}}; \[IndentingNewLine]For[k = 1, 
          k \[LessEqual] N, \(k++\), \[IndentingNewLine]X = 
            Table[ret[\([k, 1]\)] + alpha[\([i]\)]*H[\([k]\)], {i, 1, 
                Length[alpha]}]; \[IndentingNewLine]w[t_] := 
            f[X, Table[
                ret[\([k, 2]\)] + betta[\([i]\)] . t*H[\([k]\)], {i, 1, 
                  Length[alpha]}]]; \[IndentingNewLine]F = 
            FixedPoint[w, Table[ret[\([k, 2]\)], {i, 1, Length[alpha]}], 
              4]; \[IndentingNewLine]AppendTo[
            ret, {ret[\([k, 1]\)] + H[\([k]\)], 
              ret[\([k, 2]\)] + 
                F . gamma*
                  H[\([k]\)]}];\[IndentingNewLine]] (*end\ for*) ; \
\[IndentingNewLine]ret]\)\( (*end\ module*) \)\)\)], "Input"],

Cell[BoxData[
    \(\(\(AdamsBashforth[Xo_, Yo_, H_, f_, e_, alpha_, 
        M_]\)\(:=\)\(Module[{N, ret, k}, 
        N = Length[H]; \[IndentingNewLine]If[e[0, 0, 0] \[Equal] \(-10\), 
          ret = {Xo, Yo}, 
          ret\  = ExplicitRungeKutta[Xo, Yo, 
              If[N < M - 1, H, Take[H, M - 1]], e]]; \[IndentingNewLine]For[
          k = M, k \[LessEqual] N, \(k++\), \[IndentingNewLine]AppendTo[
            ret, {ret[\([k, 1]\)] + H[\([k]\)], 
              ret[\([k, 
                    2]\)] + \((\[Sum]\+\(j = 1\)\%M\((f[
                            ret[\([k + 1 - j, 1]\)], ret[\([k + 1 - j, 2]\)]]*
                          alpha[\([j]\)])\))\)*
                  H[\([k]\)]}]\[IndentingNewLine]] (*end\ for*) ; \
\[IndentingNewLine]ret]\)\( (*end\ module*) \)\)\)], "Input"],

Cell[BoxData[
    \(\(\(AdamsMoulton[Xo_, Yo_, H_, f_, e_, alpha_, 
        M_]\)\(:=\)\(Module[{N, ret, k, newy, w}, 
        N = Length[H]; \[IndentingNewLine]If[e[0, 0, 0] \[Equal] \(-10\), 
          ret = {Xo, Yo}, 
          ret\  = ExplicitRungeKutta[Xo, Yo, 
              If[N < M - 1, H, Take[H, M - 1]], e]]; \[IndentingNewLine]For[
          k = M, k \[LessEqual] N, \(k++\), \[IndentingNewLine]w[t_] := 
            ret[\([k, 2]\)] + 
              f[ret[\([k, 1]\)] + H[\([k]\)], t]*H[\([k]\)]*
                alpha[\([1]\)] + \((\[Sum]\+\(j = 1\)\%M\((f[
                          ret[\([k + 1 - j, 1]\)], ret[\([k + 1 - j, 2]\)]]*
                        alpha[\([j + 1]\)])\))\)*
                H[\([k]\)]; \[IndentingNewLine]newy = 
            FixedPoint[w, ret[\([k, 2]\)], 4]; \[IndentingNewLine]AppendTo[
            ret, {ret[\([k, 1]\)] + H[\([k]\)], 
              newy}]\[IndentingNewLine]] (*end\ for*) ; \
\[IndentingNewLine]ret]\)\( (*end\ module*) \)\)\)], "Input"],

Cell[BoxData[
    \(\(\(BDF[Xo_, Yo_, H_, f_, e_, G_, 
        M_]\)\(:=\)\(Module[{N, ret, k, newy, w}, 
        N = Length[H]; \[IndentingNewLine]If[e[0, 0, 0] \[Equal] \(-10\), 
          ret = {Xo, Yo}, 
          ret\  = ExplicitRungeKutta[Xo, Yo, 
              If[N < M - 1, H, Take[H, M - 1]], e]]; \[IndentingNewLine]For[
          k = M, k \[LessEqual] 
            N, \(k++\), \[IndentingNewLine]w[
              t_] := \((H[\([k]\)]*
                    f[ret[\([k, 1]\)] + H[\([k]\)], 
                      t] - \((\[Sum]\+\(j = 1\)\%M\((ret[\([k + 1 - j, 2]\)]*
                          G[\([j + 1]\)])\))\))\)/
              G[\([1]\)]; \[IndentingNewLine]newy = 
            FixedPoint[w, ret[\([k, 2]\)], 4]; \[IndentingNewLine]AppendTo[
            ret, {ret[\([k, 1]\)] + H[\([k]\)], 
              newy}]\[IndentingNewLine]] (*end\ for*) ; \
\[IndentingNewLine]ret]\)\( (*end\ module*) \)\)\)], "Input"],

Cell[BoxData[
    \(\(\(FehlerAuswertung[l_, y_, 
        f_]\)\(:=\)\(Module[{yk, ek, dk, dksum, k, lsg}, ek = \(yk = {}\); 
        dk = \(dksum = {0}\); \[IndentingNewLine]For[k = 1, 
          k \[LessEqual] \ Length[l], \(k++\), \[IndentingNewLine]AppendTo[
            yk, y[l[\([k, 1]\)]]]; \[IndentingNewLine]AppendTo[ek, 
            y[l[\([k, 1]\)]] - 
              l[\([k, 2]\)]]] (*end\ for*) ; \[IndentingNewLine]For[k = 2, 
          k \[LessEqual] \ Length[l], \(k++\), \[IndentingNewLine]AppendTo[
            dk, y[l[\([k, 
                    1]\)]] - \((l[\([k - 1, 
                      2]\)] + \((l[\([k, 1]\)] - l[\([k - 1, 1]\)])\)*
                    f[l[\([k - 1, 1]\)], l[\([k - 1, 2]\)], 
                      l[\([k, 1]\)] - 
                        l[\([k - 1, 1]\)]])\)]; \[IndentingNewLine]AppendTo[
            dksum, Plus @@ 
              dk]] (*end\ for*) ; \[IndentingNewLine]Transpose[{l[\([All, 
                1]\)], l[\([All, 2]\)], yk, ek, dk, 
            dksum}]]\)\( (*end\ module*) \)\)\)], "Input"],

Cell[BoxData[
